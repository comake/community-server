{
  "@context": "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^4.0.0/components/context.jsonld",
  "@graph": [
    {
      "comment": "Routes all OIDC related requests to the OIDC library.",
      "@id": "urn:solid-server:default:OidcHandler",
      "@type": "RouterHandler",
      "RouterHandler:_args_baseUrl": { "@id": "urn:solid-server:default:variable:baseUrl" },
      "RouterHandler:_args_targetExtractor": { "@id": "urn:solid-server:default:TargetExtractor" },
      "RouterHandler:_args_allowedMethods": [ "*" ],
      "RouterHandler:_args_allowedPathNames": [ "^/.oidc/.*", "^/\\.well-known/openid-configuration" ],
      "RouterHandler:_args_handler": {
        "@type": "OidcHttpHandler",
        "OidcHttpHandler:_providerFactory": { "@id": "urn:solid-server:default:IdentityProviderFactory" }
      }
    }
  ]
}
